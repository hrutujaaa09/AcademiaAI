{
    "name": "acedemiaai",
    "version": "1.0.0",
    "description": "AcedemiaAI - AI-powered educational app",
    "main": "src/main.js", 
    "scripts": {
      "start": "electron .", 
      "dev": "electron .",
      "build": "electron-builder"
    },
    "dependencies": {
      "electron": "^24.0.0", 
      "axios": "^1.3.2", 
      "material-design-icons": "^3.0.1"
    },
    "devDependencies": {
      "electron-builder": "^24.0.0",
      "electron-reload": "^1.5.0"
    },
    "keywords": ["AI", "education", "app", "AcedemiaAI"],
    "author": "Your Name",
    "license": "ISC",
    "build": {
      "appId": "com.acedemiaai.app",
      "productName": "AcedemiaAI",
      "directories": {
        "output": "dist"
      },
      "files": [
        "src/**/*",
        "assets/**/*"
      ],
      "win": {
        "target": "nsis"
      },
      "mac": {
        "category": "public.app-category.education"
      },
      "linux": {
        "target": "AppImage"
      }
    }
  }
  